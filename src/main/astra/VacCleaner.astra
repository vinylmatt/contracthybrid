agent VacCleaner extends FIPACNParticipant {    
    module Console c;
	module Math math;
    module System s;
	module EIS ei;
    module Debug d;

	 types vac_ontology {
        formula vac(string);
		formula move(string, int );
		formula free(string);
		formula location_status(long, long, string, string);
		formula lastmove(long, long, string);
		formula wall_status(string);
		formula nodust(int);
		formula wallcount(int);
		formula reset(string);
		formula binarychoice(string, string);
		formula fullchoice(string, string, string);
		
		 }

	initial nodust(0);
	initial wallcount(0);
	initial reset("no");

	inference free(string X) :- ~ei.square(X, "obstacle") & ~ei.square(X, "vac") ;
	inference binarychoice(string X, string Y) :- free(X) & free(Y) ;
	inference fullchoice(string X, string Y, string Z) :- free(X) & free(Y) & free(Z) ;

	
	
	rule +!main([string name]) {
		//c.println("Heroic worker is: " + s.name());
        //c.println("and is in comms to: " + name);
        ei.launch("vw","dependency/vacuumworld-1.2.0.jar");
        ei.join();
        ei.link(s.name());
		s.sleep(200);
    }




/*Cleaner Only*/
    rule +!generate_bid(int id, int value) : contract(id, "test", [], int deadline) & nodust(int dustturn) {
       
		value = dustturn;
	
    }

    rule +!fulfil(string id, "test", []) {
		//c.println("...................................I won "+id+" bid!!! I am the laziest robot wake up!");
		!decide();
    }

