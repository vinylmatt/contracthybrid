		!dustturn();
		!move("forward");
		+location_status(A, B, "visited", d);
		c.println("DC1.2 DEFAULT FORWARD COMPLETES "+ d +". OPEN ROAD *** DEFAULT FORWARD *** at x "+A+" y "+B+" .");
    }

