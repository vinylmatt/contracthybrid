rule +!decide() : ei.square("here", "dust") & ei.square("right", "dust") & ei.location(long A, long B) & ei.direction(string d) {
		c.println("D1-dust-here-Clean-move-Right  X "+A+" Y "+B+" facing "+d+".");
		ei.light("on");
	    !clean();
		!move("right");
    }

